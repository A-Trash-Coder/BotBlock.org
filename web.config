<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <handlers>
            <add name="iisnode" path="src/main.js" verb="*" modules="iisnode"/>
        </handlers>
        <rewrite>
            <rules>
                <rule name="node">
                    <match url="/*"/>
                    <action type="Rewrite" url="src/main.js"/>
                </rule>
            </rules>
        </rewrite>
    </system.webServer>
</configuration>
